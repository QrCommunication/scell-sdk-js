{
  "name": "@scell/sdk",
  "version": "1.4.0",
  "description": "Official TypeScript SDK for Scell.io - Electronic invoicing (Factur-X) and signatures (eIDAS) API",
  "author": "Scell.io <contact@scell.io>",
  "license": "MIT",
  "keywords": [
    "scell",
    "invoicing",
    "factur-x",
    "electronic-signature",
    "eidas",
    "api",
    "sdk",
    "typescript",
    "billing",
    "e-invoicing"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/QrCommunication/scell-sdk-js.git"
  },
  "homepage": "https://scell.io",
  "bugs": {
    "url": "https://github.com/QrCommunication/scell-sdk-js/issues"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.55.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  }
}
